{
  "name": "Twitter Info",
  "version": "1.0",
  "description": "Shows a page action for urls which have the letter 'g' in them.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "There's a 'G' in this URL!",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "declarativeContent",
    "activeTab",
    "tabs",
    "identity",
    "identity.email"
  ],
  "icons": {},
  "key": "",
  "oauth2": {
    "client_id": "208313155573-ed4sjbuflsi9qb6bjflgrmgovf1s4hdk.apps.googleusercontent.com",
    "scopes": [
      "profile email",
      //"https://www.googleapis.com/auth/"
      //"https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "manifest_version": 2
}
